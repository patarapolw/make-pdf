{
  "name": "@patarapolw/make-pdf",
  "version": "0.1.0",
  "description": "Create PDF from markdown, or HTML files",
  "keywords": [
    "pdf",
    "puppeteer",
    "pdf-lib"
  ],
  "license": "ISC",
  "author": {
    "name": "Pacharapol Withayasakpunt",
    "email": "polv@polv.cc",
    "url": "https://www.polv.cc"
  },
  "main": "lib/server.js",
  "types": "lib/server.d.ts",
  "bin": {
    "makepdf": "lib/cli.js"
  },
  "files": [
    "assets",
    "lib",
    "public",
    "src",
    "tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -P src/tsconfig.json",
    "prepack": "cp ../../README.md README.md",
    "shamefully-hoist": "echo 'shamefully-hoist=true' >> .npmrc",
    "ts": "ts-node --transpile-only"
  },
  "dependencies": {
    "@patarapolw/markdown-it-imsize": "^2.0.1-dev3",
    "dayjs": "^1.9.1",
    "domino": "^2.1.6",
    "electron": "^10.1.3",
    "express": "^4.17.1",
    "js-yaml": "^3.14.0",
    "markdown-it": "^11.0.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-external-links": "^0.0.6",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "open": "^7.3.0",
    "pdf-lib": "^1.11.1",
    "pug": "^3.0.0",
    "sass": "^1.26.11",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/js-yaml": "^3.12.5",
    "@types/markdown-it": "^10.0.2",
    "@types/morgan": "^1.9.1",
    "@types/node-fetch": "^2.5.7",
    "@types/pug": "^2.0.4",
    "@types/sass": "^1.16.0",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "12.x",
    "npm": "please-use-pnpm",
    "pnpm": "5.x",
    "yarn": "please-use-pnpm"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx, .vue": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
