{
  "name": "@patarapolw/make-pdf",
  "version": "0.1.0",
  "description": "Create PDF from markdown, or HTML files",
  "keywords": [
    "pdf",
    "markdown",
    "html2pdf",
    "markdown2pdf"
  ],
  "license": "MIT",
  "author": {
    "name": "Pacharapol Withayasakpunt",
    "email": "polv@polv.cc",
    "url": "https://www.polv.cc"
  },
  "main": "lib/server.js",
  "types": "lib/server.d.ts",
  "bin": {
    "makepdf": "lib/cli.js"
  },
  "files": [
    "assets",
    "lib",
    "public",
    "src",
    "tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./lib --extensions '.ts,.js'",
    "prepack": "cp ../../README.md README.md"
  },
  "dependencies": {
    "handlebars": "^4.7.6",
    "js-yaml": "^3.14.0",
    "puppeteer": "^5.3.1",
    "showdown": "^1.9.1",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "12.x",
    "npm": "please-use-pnpm",
    "pnpm": "5.x",
    "yarn": "please-use-pnpm"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx, .vue": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
